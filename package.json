{
  "name": "my-record-collection",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "prisma:db-push": "prisma db push --schema services/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema services/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema services/prisma/schema.prisma"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.11.0",
    "ml-kmeans": "^6.0.0",
    "next": "13.2.1",
    "next-auth": "^4.19.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.1",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "prisma": "^4.11.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ],
    "!**/*.{js,jsx,ts,tsx}": "prettier --write --ignore-unknown",
    "schema.prisma": "npm run prisma:db-push && npm run prisma:generate"
  }
}
